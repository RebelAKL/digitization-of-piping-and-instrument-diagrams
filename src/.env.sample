# Debug / logging
DEBUG=true

# Symbol detection model API
SYMBOL_DETECTION_API=<YOUR_SYMBOL_DETECTION_API>
SYMBOL_DETECTION_API_BEARER_TOKEN=<YOUR_SYMBOL_DETECTION_API_BEARER_TOKEN>

# Storage (use GCS bucket instead of Azure Blob)
GCS_BUCKET_NAME=<YOUR_GCS_BUCKET_NAME>
GCP_PROJECT=<YOUR_GCP_PROJECT_ID>

# Database (Cloud SQL MySQL)
GRAPH_DB_TYPE=mysql
CLOUD_SQL_INSTANCE_CONNECTION_NAME=<YOUR_PROJECT>:<YOUR_REGION>:<YOUR_INSTANCE>
DB_USER=<YOUR_DB_USER>
DB_PASSWORD=<YOUR_DB_PASSWORD>
DB_NAME=<YOUR_DB_NAME>

# App settings
PORT=8080

# Optional: inference tuning
INFERENCE_SCORE_THRESHOLD=0.3
SYMBOL_LABEL_PREFIXES_WITH_TEXT="Instrument/,Equipment/,Piping/Endpoint/Pagination"
SYMBOL_LABEL_PREFIXES_TO_INCLUDE_IN_GRAPH_IMAGE_OUTPUT="Equipment/,Instrument/Valve/,Piping/Endpoint/Pagination"
